GHGA:target_coverage a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "target coverage" ;
    rdfs:range linkml:String ;
    skos:definition "Mean coverage for whole genome sequencing, or mean target coverage for whole exome and targeted sequencing. The number of times a particular locus (site, nucleotide, amplicon, region) was sequenced." .

GHGA:target_regions a owl:ObjectProperty,
        linkml:SlotDefinition ;

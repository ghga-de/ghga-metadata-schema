GHGA:reference_annotation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "reference annotation" ;
    rdfs:range linkml:String ;
    skos:definition "A published genetic sequence that is used as a reference sequence against which other sequences are compared. Reference genome(s) or annotation(s) used for prior analyses (eg: GRCh38.p13)." .

GHGA:release_date a owl:ObjectProperty,
